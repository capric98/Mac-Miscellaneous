{
    "title": "Fn + Arrow Keys",
    "rules": [
        {
            "description": "Fn+↑ -> Page Up",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "up_arrow",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "caps_lock",
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_up"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Fn+↓ -> Page Down",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "down_arrow",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "caps_lock",
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_down"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Fn+← -> Home",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_arrow",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "caps_lock",
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "home"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Fn+→ -> End",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_arrow",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "caps_lock",
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "end"
                        }
                    ]
                }
            ]
        }
    ]
}